  <sql id="selectBaseCollectionSql">
    select <include refid="dto-column-list" />
    from t_collection c 
        inner join t_user_profile u
            on c.user_id = u.id
        left join t_forum f
            on c.forum_id = f.id
        left join t_post p
            on c.post_id = p.id
        left join t_topic t
            on c.topic_id = t.id
        left join t_user_profile fo 
            on c.following_id = fo.id
        left join t_commody cm
            on c.commody_id = cm.id 
  </sql>
